<%- include header %>
<section id="main">
    <div class="container">
        <div class="row">
            <div class="col-md-10 col-md-offset-1">
                <% var lastYear = 0; %>
                <% if(posts[0]){ %>
                    <h3>Results match "<%= title %>"</h3>
                    <% posts.forEach(function(post, index){  %>
                    <% if(lastYear != post.time.year) { %>
                    <p>
                    <h3><%= post.time.year %></h3>
                    </p>
                    <% lastYear = post.time.year } %>
                    <div class="well">
                        <h3><a href="/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a></h3>
                        <p class="grey"><%= post.time.date %> â€” <%= post.name %></p>
                    </div>
                    <% }) %>
                <% }else{ %>
                <div class="well">
                    <h3>No results match "<%= title %>"</h3>
                </div>
                <% } %>
            </div>
        </div>
    </div>
</section>
<%- include footer %>
